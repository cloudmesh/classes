InProceedings{author = {Engle and Cliff and Lupher and Antonio and Xin and Reynold and Zaharia and Matei and Franklin and Michael J. and Shenker and Scott and Stoica and Ion},
title = {Shark: Fast Data Analysis Using Coarse-grained Distributed Memory},
booktitle = {Proceedings of the 2012 ACM SIGMOD International Conference on Management of Data},
series = {SIGMOD '12},
year = {2012},
isbn = {978-1-4503-1247-9},
location = {Scottsdale, Arizona, USA},
pages = {689--692},
numpages = {4},
url = {http://doi.acm.org/10.1145/2213836.2213934},
doi = {10.1145/2213836.2213934},
acmid = {2213934},
